{
  extends: [
    'github>MareMare/renovate-config',
  ],
  packageRules: [
    {
      description: 'Automerge non-major updates',
      matchUpdateTypes: [
        'minor',
        'patch',
        'pin',
        'digest',
      ],
      automerge: true,
    },
    {
      groupName: 'microsoft-identity-web',
      matchSourceUrls: [
        'https://github.com/AzureAD/microsoft-identity-web{/,}**',
      ],
    },
  ],
  "dotnet": {
    "enabled": true,
    "constraints": {
      "sdk": "8.0.404"
    },
    "binarySource": "install"
  }
}
